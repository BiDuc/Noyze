    <!-- App description, similar to text in Google Play. -->
    <string name="description">Noyze is the customizable volume panel for Android with themes, media playback controls, and much more!</string>

    <!-- Accessibility text to explain the need for this service. -->
    <string name="accessibility_service_label">Noyze Service</string>
    <string name="accessibility_service_description">Accessibility allows noyze to control the volume buttons.
        Only info needed to recreate the volume panel is processed, and absolutely none of this info is stored or shared.</string>
    <string name="accessibility_dialog_title">Accessibility</string>
    <string name="accessibility_dialog_description">noyze needs to be enabled in Settings > Accessibility to control the volume buttons.</string>
    <string name="accessibility_dialog_description_on">noyze can only be disabled in Settings > Accessibility.</string>
    <string name="accessibility_error">Cannot open: go to Settings > Accessibility</string>

